<h1> {{ siteName }}</h1>
<nav class="nav-bar" role="navigation">
    <h2 class="offscreen">Main navigation</h2>
    <div class="logo"></div>
    <ul id="navigation-list" class="navigation-list">

        <!-- Iterate through the navItems and extract each link href and title -->
        {{#each ./navItems}}

            {{#is ./class "active"}}
                <li><span>{{./title}}</span></li>
            {{else}}
                <li><a class="{{ ./class }}"  href="{{ ./href }}">{{ ./title }}</a></li>
            {{/is}}

        {{/each}}

    </ul>
</nav>

<!-- Hamburger Button-->
<button id="navbar-toggle" type="button" class="navbar-toggle">
    <!--<span class="sr-only">Toggle navigation</span>-->
    <span></span>
    <span></span>
    <span></span>
    <span></span>
</button>